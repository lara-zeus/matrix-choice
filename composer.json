{
    "name": "lara-zeus/matrix-choice",
    "description": "Zeus Matrix Choice multiple choice grid component form for filamentPHP",
    "keywords": [
        "laravel",
        "lara-zeus",
        "matrix",
        "choice",
        "grid",
        "form",
        "forms",
        "feilds",
        "generator",
        "self hosted",
        "maker",
        "builder",
        "filamentphp"
    ],
    "homepage": "https://larazeus.com/matrix-choice",
    "support": {
        "issues": "https://github.com/lara-zeus/matrix-choice/issues",
        "source": "https://github.com/lara-zeus/matrix-choice"
    },
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "php coder",
            "email": "info@larazeus.com",
            "role": "Owner"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "laravel/pint": "^1.0",
        "larastan/larastan": "^2.2",
        "orchestra/testbench": "^8.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "LaraZeus\\MatrixChoice\\": "src"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "pint": "vendor/bin/pint",
        "test:phpstan": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraZeus\\MatrixChoice\\MatrixChoiceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
